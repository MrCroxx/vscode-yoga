{
  "name": "yoga",
  "displayName": "Yoga",
  "description": "Yet another leap.nvim for vscode.",
  "version": "0.0.2",
  "publisher": "MrCroxx",
  "license": "MIT",
  "icon": "assets/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrcroxx/vscode-yoga"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "keywords": [
    "leap",
    "leap.nvim",
    "vim",
    "neovim",
    "jump",
    "jumpy",
    "cursor",
    "find",
    "finder",
    "quick",
    "rapid",
    "move",
    "movement",
    "navigation",
    "util",
    "utility",
    "efficiency",
    "shortcut"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yoga.find",
        "title": "Yoga Find"
      },
      {
        "command": "yoga.findWithCase",
        "title": "Yoga Find (Case Sensitive)"
      },
      {
        "command": "yoga.findEnd",
        "title": "Yoga Find End"
      },
      {
        "command": "yoga.findEndWithCase",
        "title": "Yoga Find End (Case Sensitive)"
      }
    ],
    "keybindings": [
      {
        "command": "yoga.find",
        "key": "ctrl+alt+s"
      },
      {
        "command": "yoga.findWithCase",
        "key": "ctrl+alt+shift+s"
      },
      {
        "command": "yoga.findEnd",
        "key": "ctrl+alt+d"
      },
      {
        "command": "yoga.findEndWithCase",
        "key": "ctrl+alt+shift+d"
      }
    ],
    "configuration": [
      {
        "title": "Yoga",
        "properties": {
          "yoga.activeEditorOnly": {
            "type": "boolean",
            "default": false,
            "description": "Only find matches in the active text editor."
          },
          "yoga.alphabet": {
            "type": "string",
            "default": "abcdefghijklmnopqrstuvwxyz",
            "description": "Alphabet for label generation."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}